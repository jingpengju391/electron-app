{
  "name": "5g-app",
  "version": "1.0.0",
  "description": "An Electron application with Vue and TypeScript",
  "main": "./out/main/index.js",
  "author": "aojiaoshou",
  "homepage": "https://electron-vite.org",
  "scripts": {
    "format": "prettier --write .",
    "figlet": "node scripts/figlet.js",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts,.vue --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "vue-tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "pnpm typecheck:node && pnpm typecheck:web",
    "start": "electron-vite preview",
    "dev": "electron-vite dev -w",
    "build": "pnpm typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:unpack": "pnpm build && electron-builder --dir",
    "build:win": "pnpm build && electron-builder --win",
    "build:mac": "pnpm build && electron-builder --mac",
    "build:linux": "pnpm build && electron-builder --linux",
    "preinstall": "npx only-allow pnpm",
    "build:linux:win": "docker-compose -f docker/docker-compose.yml up --build"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.0",
    "@electron-toolkit/utils": "^3.0.0",
    "@element-plus/icons-vue": "^2.3.1",
    "@vitejs/plugin-legacy": "^5.4.1",
    "axios": "^1.7.2",
    "electron-updater": "^6.1.7",
    "element-plus": "^2.8.6",
    "element-resize-detector": "^1.2.4",
    "knex": "^3.1.0",
    "moment": "^2.30.1",
    "pinia": "^2.1.7",
    "sqlite3": "^5.1.7",
    "vue-router": "^4.4.0"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config": "^1.0.2",
    "@electron-toolkit/eslint-config-ts": "^2.0.0",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@iconify-json/ep": "^1.2.1",
    "@rushstack/eslint-patch": "^1.10.3",
    "@types/node": "^20.14.8",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "electron": "31.3.1",
    "electron-builder": "^24.13.3",
    "electron-vite": "^2.3.0",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.26.0",
    "figlet": "^1.7.0",
    "only-allow": "^1.2.1",
    "prettier": "^3.3.2",
    "sass": "^1.80.3",
    "sass-loader": "^16.0.2",
    "typescript": "^5.5.2",
    "unplugin-auto-import": "^0.18.3",
    "unplugin-icons": "^0.19.3",
    "unplugin-vue-components": "^0.27.4",
    "vite": "^5.3.1",
    "vite-plugin-inspect": "^0.8.7",
    "vite-plugin-svg-icons": "^2.0.1",
    "vue": "^3.5.12",
    "vue-tsc": "^2.0.22"
  }
}
